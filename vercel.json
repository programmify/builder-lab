{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/guides/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/examples/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|_next/static|_next/image|assets|favicon.ico|sw.js).*)",
      "destination": "/index.html"
    }
  ]
}
